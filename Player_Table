CREATE TABLE Player (
  id varchar(16),
  name varchar(40),
  team_id varchar(16),
  position varchar(8),
  squad_no numeric(3,0),
  height numeric (8,2),
  weight numeric (8,2),
  rebounds numeric (8,2),
  assists numeric (8,2),
  steals numeric (8,2),
  blocks numeric (8.2),
  twopoint_shot_percentage numeric (8,2),
  threepoint_shot_percentage numeric (8,2),
  start_year numeric(4,0),
  PRIMARY KEY (id),
  FOREIGN KEY (team_id),
);
